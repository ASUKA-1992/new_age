<!DOCTYPE html>
<html>
  <head>
    <title>新しい10年間</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <header>
      <% unless request.url[-1] == "\/" %>
        <%= render "shared/header" %>
      <% end %>
    </header>
    <%= yield %>
  </body>
</html>
