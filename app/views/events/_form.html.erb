<%= javascript_include_tag "event_new_edit.js" %>
<div class="body">
  <div class="div_center">
    <div class="div_bold">■出来事情報入力画面■</div><br/>
    <%= link_to "戻る", @event, :class=>'no_border_link' %>
    <div><span class="div_bold">出来事情報を入力してください</span></div>
    <%= render "shared/errors", obj: @event %>
    <%= form_for @event do |form| %>

      <table id="age_new_table" class="table_main inline-block">
        <tbody class="table_body">
          <tr>
            <th class="td_w100">
              見出し<span class="font_9 font_wight_lighter"> 25文字以内</span>
            </th>
            <td colspan="3" class="div_left"><%= form.text_field :title, size:65 %></td>
            <th class="td_w75">日付</th>
            <td class="div_left" id="saelect_date">
              <%= form.date_select  :date,
                  start_year: 2011, end_year: 2020,
                  use_month_numbers: true, :id=>'a' %>
            </td>
            <th class="td_w75">総人口増減</th>
            <td class="div_left">
              <%= form.text_field :population_change, size: 20, :class=>'age_new_input' %>
            </td>
          </tr>
          <tr>
            <th class="">
              詳細<span class="font_9 font_wight_lighter"> 200文字以内</span>
            </th>
            <td colspan="5" class=""><%= form.text_area :detail, cols: 95, rows: 5 %></td>
            <td colspan="2">
              <%= form.radio_button :is_fake, false %>
              事実
              <%= form.radio_button :is_fake, true %>
              フィクション <br/>
              <% if @admin_flg || controller.check_first_data(@event) %>
                <%= form.radio_button :is_active, true %>
                有効
                <%= form.radio_button :is_active, false %>
                無効
              <% end %>
            </td>
          </tr>
          <tr>
            <td class="font_12">
              <p class="div_bold">汚染・沈没設定</p>
                ■ボタン説明■<br/>
